{
    "name": "maxbrokman/safe-queue",
    "description": "A Laravel Doctrine friendly daemonising queue worker for Laravel 6",
    "license": "MIT",
    "authors": [
        {
            "name": "Max Brokman",
            "email": "max.brokman@vice.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/queue": "^6.0|^7.0",
        "laravel-doctrine/orm": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "mockery/mockery": "^0.9.4",
        "satooshi/php-coveralls": "^2.2",
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "autoload": {
        "psr-4": {
            "MaxBrokman\\SafeQueue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "test\\MaxBrokman\\SafeQueue\\": "tests/"
        }
    }
}
